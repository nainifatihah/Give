<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient2"
    android:paddingBottom="7dp"
    android:paddingLeft="14dp"
    android:paddingRight="7dp"
    android:paddingTop="7dp"
    tools:context="com.ainifathiha.give.HomeActivity">

    <LinearLayout
        android:layout_width="332dp"
        android:layout_height="564dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"
        android:weightSum="1">

        <ImageView
            android:id="@+id/ivGiveLogo"
            android:layout_width="match_parent"
            android:layout_height="88dp"
            app:srcCompat="@drawable/logo_give2"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"
            android:layout_weight="0.91" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ivGive1"
                android:layout_width="match_parent"
                android:layout_height="369dp"
                android:layout_gravity="left"
                app:srcCompat="@drawable/thrift"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="-2dp"
                tools:layout_editor_absoluteY="66dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/ibHome"
                android:layout_width="66dp"
                android:layout_height="66dp"
                android:layout_weight="0.2"
                android:background="@drawable/icon_home"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="412dp" />

            <ImageButton
                android:id="@+id/ibSearch"
                android:layout_width="66dp"
                android:layout_height="66dp"
                android:layout_weight="0.2"
                android:background="@drawable/icon_search"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="71dp"
                tools:layout_editor_absoluteY="412dp" />

            <ImageButton
                android:id="@+id/ibCamera"
                android:layout_width="66dp"
                android:layout_height="66dp"
                android:layout_weight="0.2"
                android:background="@drawable/icon_camera"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="141dp"
                tools:layout_editor_absoluteY="412dp" />

            <ImageButton
                android:id="@+id/ibChat"
                android:layout_width="66dp"
                android:layout_height="66dp"
                android:layout_weight="0.2"
                android:background="@drawable/icon_chat"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="212dp"
                tools:layout_editor_absoluteY="412dp" />

            <ImageButton
                android:id="@+id/ibProfile"
                android:layout_width="66dp"
                android:layout_height="66dp"
                android:layout_weight="0.2"
                android:background="@drawable/icon_account"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="281dp"
                tools:layout_editor_absoluteY="412dp" />
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>
